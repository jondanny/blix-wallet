{
  "name": "blix-wallet",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --maxWorkers=1 --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --forceExit",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand --forceExit",
    "migration:run": "NODE_ENV=development npx typeorm-ts-node-commonjs migration:run --dataSource ./src/config/datasource.ts",
    "migration:run:prod": "NODE_ENV=production npx typeorm-ts-node-commonjs migration:run --dataSource ./src/config/datasource.ts",
    "migration:run:ci": "NODE_ENV=test npx typeorm-ts-node-commonjs migration:run --dataSource ./src/config/datasource.ts",
    "migration:revert": "dotenv -e .env.development ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:revert",
    "migration:generate": "dotenv -e .env.development ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:generate --",
    "migration:create": "dotenv -e .env.development ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:create --",
    "repl": "npm run start -- --entryFile repl",
    "hardhat:compile": "npx hardhat compile",
    "hardhat:node": "npx hardhat node",
    "hardhat:deploy:nft": "npx hardhat run hardhat/deploy/DigikraftNft.deploy.ts --network localhost",
    "hardhat:deploy:nft:mumbai": "npx hardhat run hardhat/deploy/DigikraftNft.deploy.ts --network mumbai_testnet",
    "hardhat:deploy:nft:polygon": "npx hardhat run hardhat/deploy/DigikraftNft.deploy.ts --network polygon_mainnet",
    "hardhat:deploy:nft:goerli": "npx hardhat run hardhat/deploy/DigikraftNft.deploy.ts --network goerli_testnet",
    "hardhat:deploy:adminlist:mumbai": "npx hardhat run hardhat/deploy/DigikraftAdminList.deploy.ts --network mumbai_testnet",
    "hardhat:deploy:adminlist:polygon": "npx hardhat run hardhat/deploy/DigikraftAdminList.deploy.ts --network polygon_mainnet",
    "hardhat:deploy:adminlist:goerli": "npx hardhat run hardhat/deploy/DigikraftAdminList.deploy.ts --network goerli_testnet",
    "hardhat:verify:nft:numbai": "npx hardhat run hardhat/verify/DigikraftNft.verify.ts --network mumbai_testnet",
    "hardhat:verify:nft:polygon": "npx hardhat run hardhat/verify/DigikraftNft.verify.ts --network polygon_mainnet",
    "hardhat:verify:adminlist:mumbai": "npx hardhat run hardhat/verify/DigikraftAdminList.verify.ts --network mumbai_testnet",
    "hardhat:verify:adminlist:polygon": "npx hardhat run hardhat/verify/DigikraftAdminList.verify.ts --network polygon_mainnet"
  },
  "dependencies": {
    "@alch/alchemy-web3": "^1.4.7",
    "@nestjs/bull": "^0.6.2",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/typeorm": "^9.0.1",
    "@sentry/integrations": "^7.21.1",
    "@sentry/node": "^7.21.1",
    "axios": "^1.2.0",
    "bull": "^4.10.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.11",
    "typeorm-cursor-pagination": "^0.10.0",
    "typeorm-naming-strategies": "^4.1.0",
    "web3": "^1.8.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@openzeppelin/contracts": "^4.8.0",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^6.6.9",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.12.5",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typechain": "^8.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
